{
  "driver_config": {
    "driver": "MySQL",
    "host": "proxysql-prod.srg-it.ru",
    "user": "ro_dev",
    "password": "oobeeN9g"
  },
  "query_config": {
    "fields_mapping": {
      "id": "rfv.id",
      "created_on": "rfv.created_on",
      "decision": "rfv.status",
      "valuation_type": "rp.dtype",
      "exception_type": "vr.exceptional_reason",
      "company_title": "comp.title",
      "company_id": "comp.id",
      "expert_status": "eresp.status",
      "wrong_req_type": "eresp.expert_wrong_request_type",
      "is_request_invalid": "eresp.is_request_invalid",
      "total_space": "rp.total_space",
      "report_price": "rfv.report_price / rp.total_space",
      "price": "vr.price / rp.total_space",
      "expert_price": "eresp.price / rp.total_space",
      "deviation": "vr.deviation * 100",
      "abs_deviation": "abs(vr.deviation) * 100",
      "region_title": "r.fias_title",
      "region_shortname": "r.element_type",
      "region_code": "r.code",
      "city_shortname": "c.element_type",
      "city_title": "c.fias_title",
      "city_id": "c.fias_id",
      "street_id": "s.fias_id",
      "street_title": "s.fias_title",
      "house_number": "rp.house_number",
      "lat": "rp.lat",
      "lon": "rp.lon",
      "rooms": "rp.rooms",
      "repairs": "rp.repairs",
      "repairs_new_gradation": "(SELECT CASE rp.qualified_repairs_state WHEN 'WITHOUT_REPAIRS' THEN 'needsCapital' WHEN 'SIMPLE' THEN 'needsCosmetic' WHEN 'PREPARED' THEN 'finishing' WHEN 'GOOD' THEN 'good' WHEN 'EURO' THEN 'evro' WHEN 'EXCELLENT' THEN 'excellent' END)",
      "floor": "rp.floor",
      "storeys": "rp.storeys",
      "walls": "(SELECT CASE rp.walls WHEN 0 THEN 'panel' WHEN 1 THEN 'kirpich' WHEN 2 THEN 'wooden' WHEN 3 THEN 'monolith' WHEN 5 THEN 'monolithKirpich' WHEN 4 THEN 'block' END)",
      "built_year": "year(rp.build_date)",
      "from_ba": "ir.from_ba"
    },
    "tables": {
      "request_for_valuation": {
        "short": "rfv",
        "joines": {
          "residential_property": "rp.id = rfv.realty_id",
          "valuation_result": "vr.id = rfv.id",
          "income_report": "ir.valuation_request_id = rfv.id",
          "company": "comp.id = rfv.company_id",
          "expert_request": "erq.id = rfv.id",
          "expert_response": "+expert_request"
        }
      },
      "residential_property": {
        "short": "rp",
        "joines": {
          "city": "c.fias_id = rp.city_fiasId",
          "street": "s.fias_id = rp.street_fiasId"
        }
      },
      "expert_request": {
        "short": "erq",
        "joines": {
          "request_for_valuation": "erq.id = rfv.id",
          "expert_response": "eresp.id = erq.expert_response_id"
        }
      },
      "valuation_result": {
        "short": "vr",
        "joines": {
          "request_for_valuation": "vr.id = rfv.id"
        }
      },
      "company": {
        "short": "comp",
        "joines": {
          "request_for_valuation": "comp.id = rfv.company_id"
        }
      },
      "expert_response": {
        "short": "eresp",
        "joines": {
          "expert_request": "eresp.id = erq.expert_response_id"
        }
      },
      "city": {
        "short": "c",
        "joines": {
          "residential_property": "c.fias_id = rp.city_fiasId",
          "region": "r.fias_id = c.region_fiasId"
        }
      },
      "street": {
        "short": "s",
        "joines": {
          "residential_property": "s.fias_id = rp.street_fiasId"
        }
      },
      "region": {
        "short": "r",
        "joines": {
          "city": "r.fias_id = c.region_fiasId"
        }
      },
      "income_report": {
        "short": "ir",
        "joines": {
          "residential_property": "ir.valuation_request_id = rfv.id"
        }
      }
    }
  }
}